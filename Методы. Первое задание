using System;


namespace Методы_первое
{
    class Program
    {
        public static string proverka_texta(string stroka)
        {
            for (int i = 0; i != stroka.Length; i++)
            {
                if (stroka[i] == 'e')
                { 
                    return ("V texte est' latinkaya bukva!");
                }
                
            }
            return ("V texte net latinskih bukv!");
    }
    static void Main()
        {
            Console.WriteLine("Vvedite text : ");
            string vvod = Console.ReadLine();

            Console.WriteLine(proverka_texta(vvod));
        }
    }
}
